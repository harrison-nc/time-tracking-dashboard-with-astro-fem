import timeFrames from "../../data/data.json";
---

<BasicLayout title="Time Tracking - Daily">
  <Dashboard>
    {
      timeFrames.map(function ({ title, current, previous, timeFrame }) {
        return <DashboardItem {title} {current} {previous} {timeFrame} />;
      })
    }
  </Dashboard>
</BasicLayout>
